
@model IEnumerable<WebUI.ViewModels.TestViewModel>
@{
    ViewBag.Title = "TestEdit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            @Html.Display("Questions")
        </th>
        <th>
            @Html.Display("Answers")
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)

            </td>

            <td>
                @foreach (var elem in item.Questions)
                {

                    @Html.DisplayFor(modelItem => elem.Value)

                    <br />
                }

            </td>
            <td>
                @foreach (var elem in item.Answers)
                {
                    @Html.DisplayFor(modelItem => elem.Value)

                    <br />
                }
            </td>
        </tr>
    }

</table>







